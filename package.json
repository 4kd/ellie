{
  "name": "ellie-client",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": "6.9.0"
  },
  "dependencies": {
    "autoprefixer": "6.6.0",
    "codemirror": "5.20.2",
    "css-loader": "0.25.0",
    "elm-css-webpack-loader": "2.0.2",
    "elm-webpack-loader": "4.0.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "^2.26.0",
    "list": "0.9.5",
    "postcss-loader": "1.2.1",
    "rimraf": "2.5.4",
    "serviceworker-loader": "0.1.0",
    "string-replace-webpack-plugin": "0.0.4",
    "style-loader": "0.13.1",
    "webpack": "1.13.3",
    "webpack-dashboard": "0.2.0",
    "webpack-dev-server": "1.16.2",
    "webpack-md5-hash": "0.0.5"
  },
  "scripts": {
    "watch": "webpack-dev-server --watch --config ./webpack.dev.config.js",
    "clean": "rimraf ./build",
    "build:target:env": "webpack --config ./webpack.prod.config.js",
    "build:target:production": "API_BASE=https://api.ellie-app.com/v1 EDITOR_BASE=https://ellie-app.com CDN_BASE=https://cdn.ellie-app.com EMBED_BASE=https://embed.ellie-app.com yarn build:target:env",
    "build:editor:production": "BUILD_TARGET=editor yarn run build:target:production",
    "build:embed:production": "BUILD_TARGET=embed yarn run build:target:production",
    "build:production": "yarn run clean && yarn build:editor:production && yarn build:embed:production"
  }
}
